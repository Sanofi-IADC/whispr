{
  "name": "@sanofi/whispr",
  "version": "1.3.3",
  "private": true,
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier-eslint --write $PWD/{src,tests}/**/*.ts",
    "format:check": "prettier-eslint --list-different $PWD/{src,tests}/**/*.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "npm run lint:es",
    "lint:ts": "tslint --fix -p tsconfig.json -c tslint.json",
    "lint:es": "eslint --max-warnings=55 --fix --ext .js,.ts src",
    "test": "npm run test:unit:cov && npm run test:e2e",
    "test:unit:watch": "jest --watch",
    "test:unit:cov": "jest --coverage",
    "test:unit:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.json",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs"
  },
  "dependencies": {
    "@apollo/gateway": "0.16.9",
    "@hapi/joi": "17.1.1",
    "@nestjs/common": "7.2.0",
    "@nestjs/core": "7.2.0",
    "@nestjs/graphql": "7.4.6",
    "@nestjs/mongoose": "7.0.1",
    "@nestjs/platform-fastify": "7.2.0",
    "@nestjs/platform-socket.io": "7.2.0",
    "@nestjs/websockets": "7.2.0",
    "@types/graphql": "14.5.0",
    "amazon-cognito-identity-js": "4.3.3",
    "apollo-server-fastify": "2.15.0",
    "aws-sdk": "2.707.0",
    "axios": "0.19.2",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "cross-fetch": "3.0.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "fastify-compress": "1.1.0",
    "graphql": "15.2.0",
    "graphql-redis-subscriptions": "2.2.1",
    "graphql-subscriptions": "1.1.0",
    "graphql-tag-pluck": "0.8.7",
    "graphql-tools": "6.0.11",
    "graphql-type-json": "0.3.2",
    "ioredis": "4.17.3",
    "lodash": "4.17.15",
    "mongoose": "5.9.20",
    "reflect-metadata": "0.1.13",
    "request": "2.88.2",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "superagent": "5.2.2",
    "superagent-promise": "1.1.0",
    "superagent-proxy": "2.0.0",
    "vuepress": "1.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@nestjs/cli": "7.4.1",
    "@nestjs/schematics": "7.0.1",
    "@nestjs/testing": "7.2.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.7",
    "@types/express": "4.17.7",
    "@types/hapi__joi": "17.1.3",
    "@types/jest": "26.0.3",
    "@types/mongoose": "5.7.29",
    "@types/node": "12.12.7",
    "@types/socket.io": "2.1.8",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "7.3.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "prettier-eslint-cli": "5.0.0",
    "semantic-release": "17.1.1",
    "supertest": "4.0.2",
    "ts-jest": "26.1.1",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "typescript": "3.9.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run format:check"
    }
  }
}
