    "graphql-redis-subscriptions": "2.4.0",
    "graphql-subscriptions": "1.2.1",
    "graphql-tag-pluck": "0.8.7",
    "graphql-tools": "7.0.5",
    "graphql-type-json": "0.3.2",
    "graphql-upload": "12.0.0",
    "haiku-random": "1.0.0",
    "ioredis": "4.27.10",
    "joi": "17.4.0",
    "lodash": "4.17.21",
    "mongoose": "5.13.13",
    "mongoose-cast-aggregation": "0.2.1",
    "reflect-metadata": "0.1.13",
    "request": "2.88.2",
    "rimraf": "3.0.2",
